<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.Hugo.goods.mapper.BrandMapper">
    <!-- 根据第三方商品ID跟来源查询商品信息 -->
    <select id="queryBrandList" resultType="com.Hugo.goods.pojo.Brand">
        SELECT *
        FROM tb_brand
    </select>

    <insert id="insertBrand">
        insert into tb_brand (id,name,image)
        value (#{id},#{name},#{image})
    </insert>
</mapper>